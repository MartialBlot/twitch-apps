{
  "name": "twitch-overlay",
  "version": "1.0.0",
  "description": "Twitch overlay",
  "main": "electron/main.ts",
  "scripts": {
    "start:dev": "start npm run build:dev && electron .\"",
    "start:server": "nodemon server/server.js",
    "build:dev": "rm -rf dist && webpack --mode development --config webpack.server.config.js && webpack --mode development --config webpack.dev.config.js",
    "build:prod": "rm -rf dist && webpack --mode production --config webpack.server.config.js && webpack --mode production --config webpack.prod.config.js && npm run build:native",
    "build:native": "electron-packager . twitch-overlay --platform=win32 --arch=x64 --overwrite"
  },
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "file-loader": "^6.2.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-tsparticles": "^1.37.3",
    "socket.io": "^4.3.2",
    "socket.io-client": "^4.3.2",
    "url-loader": "^4.1.1",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.5.0",
    "@types/node": "^16.11.7",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "@webpack-cli/serve": "1.6.0",
    "electron": "15.3.1",
    "electron-packager": "^15.4.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^2.0.14",
    "ts-loader": "^5.3.1",
    "typescript": "4.4.4",
    "webpack": "^4.27.1",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "^4.4.0",
    "webpack-node-externals": "3.0.0",
    "babel-loader": "8.2.3",
    "@babel/preset-env": "7.16.0"
  },
  "author": "Martial BLOT"
}